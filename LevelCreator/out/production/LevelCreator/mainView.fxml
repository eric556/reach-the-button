<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Label?>
<VBox prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller">
    <MenuBar>
        <Menu text="File">
            <MenuItem fx:id="loadButton" onAction="#loadButtonClick" text="Load"/>
            <MenuItem fx:id="newButton" onAction="#newButtonClick" text="New" />
            <MenuItem fx:id="saveButton" onAction="#newSaveClick" text="Save" />
            <MenuItem fx:id="exportButton" onAction="#exportClick" text="Export" />
        </Menu>
    </MenuBar>
    <Canvas fx:id="canvas" height="695.0" onMouseClicked="#mouseClickHandler" width="1280.0" />
    <Label fx:id="modeLabel">Mode: </Label>
</VBox>
